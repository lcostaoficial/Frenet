<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FRENET</title>
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback" />
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" />
    <link rel="stylesheet" href="~/lib/plugins/fontawesome-free/css/all.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/adminlte.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/custom.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/plugins/overlayScrollbars/css/OverlayScrollbars.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/plugins/sweetalert2/sweetalert2.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.css" asp-append-version="true" />

    @RenderSection("Styles", required: false)
</head>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <ul class="navbar-nav w-100 d-flex justify-content-between me-5">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li>
                    <ul class="navbar-nav w-100 d-flex justify-content-between align-items-center">
                        <li class="nav-item d-none d-sm-inline-block">
                            <span class="px-5"><small>Desenvolvido por: José Lucas da Silva Costa</small></span>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <aside class="main-sidebar sidebar-dark-primary elevation-4" id="sidebar-main-menu">
            <h2 class="text-center brand-link">FRENET</h2>
            <div class="sidebar">
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" role="menu">
                        @await Html.PartialAsync("_Menu")
                    </ul>
                </nav>
            </div>
        </aside>

        <div class="content-wrapper">
            <main role="main" class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </main>
        </div>
        <aside class="control-sidebar control-sidebar-dark"></aside>
        <partial name="_Footer" />
    </div>

    <script src="~/lib/plugins/jquery/jquery.min.js"></script>
    <script src="~/lib/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <script src="~/js/adminlte.js"></script>
    <script src="~/lib/plugins/sweetalert2/sweetalert2.js"></script>

    @RenderSection("Scripts", required: false)
    @RenderSection("Modals", required: false)
</body>
</html>
